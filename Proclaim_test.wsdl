<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:tns="http://proclaim.api/soap"
                  targetNamespace="http://proclaim.api/soap">

  <wsdl:types>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
               targetNamespace="http://proclaim.api/soap">

      <xs:element name="GetDocumentRequest">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="sessionToken" type="xs:string"/>
            <xs:element name="documentCode" type="xs:string"/>
            <xs:element name="format" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>

      <xs:element name="GetDocumentResponse">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="content" type="xs:base64Binary"/>
            <xs:element name="filename" type="xs:string"/>
